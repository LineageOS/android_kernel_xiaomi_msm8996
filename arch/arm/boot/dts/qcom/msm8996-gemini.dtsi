	pwm_ir {
		compatible = "pwm-ir";
		pwms = <&pm8994_pwm_2 0 0>;
	};

	qcom,msm-ssc-sensors {
		qcom,firmware-name = "slpi_a1";
	};

	sound-9335 {
		spkr-id-gpio = <&pm8994_gpios 3 0>;

		/delete-property/ qcom,wsa-max-devs;
		/delete-property/ qcom,wsa-devs;
		/delete-property/ qcom,wsa-aux-dev-prefix;
	};
};

&pm8994_gpios {
	/delete-node/ gpio@c600;
	gpio@c600 { /* GPIO 7 - KEYPAD_LED_EN */
		qcom,mode = <1>;		/* DIGITAL OUT */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <1>;		/* output hight initially */
		qcom,vin-sel = <2>;		/* 1.8 */
		qcom,src-sel = <0>;		/* CONSTANT */
		qcom,out-strength = <1>;	/* Low drive strength */
		qcom,master-en = <1>;		/* ENABLE GPIO */
		status = "okay";
	};
};

&pmi8994_haptics {
	status = "disable";
};

&slim_msm {
	tasha_codec {
		/delete-node/ swr_master;
	};
};
