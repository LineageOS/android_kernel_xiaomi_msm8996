// msm-pmi8994.dtsi start
&pmi8994_charger {
	qcom,fastchg-current-ma = <3000>;
	qcom,fastchg-current-comp = <1200>;
};
// msm-pmi8994.dtsi end

// msm8996.dtsi start
&qusb_phy0 {
	qcom,qusb-phy-init-seq = <0x38 0x80
				0x7b 0x84
				0x93 0x88
				0xcf 0x8c
				0x30 0x8
				0x79 0xc
				0x21 0x10
				0x14 0x9c
				0x9f 0x1c
				0x0 0x18>;
};

&qusb_phy1 {
	qcom,qusb-phy-init-seq = <0x38 0x80
				0x7b 0x84
				0x93 0x88
				0xcf 0x8c
