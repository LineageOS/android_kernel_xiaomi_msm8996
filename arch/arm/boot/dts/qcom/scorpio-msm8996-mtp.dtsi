&pm8994_gpios {
	gpio@c600 {
		qcom,mode = <1>;		/* Digital out */ qcom,pull = <5>;		/* NO PULL */
		qcom,vin-sel = <2>;		/* Logical 1 voltage value 1.8v */
		qcom,src-sel = <2>;		/* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,pull = <5>;		/* NO PULL */
		qcom,out-strength = <1>;/* Low drive strength */
		qcom,master-en = <1>;
		status = "okay";
	};

	gpio@c900 {
		qcom,mode = <1>;		/* Digital out */ qcom,pull = <5>;		/* NO PULL */
		qcom,vin-sel = <2>;		/* Logical 1 voltage value 1.8v */
		qcom,src-sel = <2>;		/* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,pull = <5>;		/* NO PULL */
		qcom,out-strength = <1>;/* Low drive strength */
		qcom,master-en = <1>;
		status = "okay";
	};
};

&pm8994_0 {
	qcom,leds@c600 {
				compatible = "qcom,leds-qpnp";
				reg = <0xc600 0x100>;
				status = "okay";
				qcom,led_gpio_7 {
						label = "kpdbl";
						linux,name = "button-backlight";
						qcom,mode = "pwm";
						pwms = <&pm8994_pwm_3 0 0>;
						qcom,pwm-us = <1000>;
						qcom,id = <7>;
						qcom,max-current = <5>;
						qcom,row-id = <0>;
						qcom,row-src-en;
						qcom,start-idx = <1>;
						qcom,ramp-step-ms = <120>;
						qcom,idx-len = <22>;
						qcom,duty-pcts = [00
						00 03 0b 19 2a
						3b 4b 59 61 64
						64 61 59 4b 3b
						2a 19 0b 03 00
						00];
						qcom,use-blink;
						qcom,in-order-command-processing;
				};
	};

	qcom,leds@c900 {
				compatible = "qcom,leds-qpnp";
				reg = <0xc900 0x100>;
				status = "okay";
				qcom,led_gpio_10 {
						label = "kpdbl";
						linux,name = "button-backlight1";
						qcom,mode = "pwm";
						pwms = <&pm8994_pwm_6 0 0>;
						qcom,pwm-us = <1000>;
						qcom,id = <7>;
						qcom,max-current = <5>;
						qcom,row-id = <0>;
						qcom,row-src-en;
						qcom,start-idx = <1>;
						qcom,ramp-step-ms = <120>;
						qcom,idx-len = <22>;
						qcom,duty-pcts = [00
						00 03 0b 19 2a
						3b 4b 59 61 64
						64 61 59 4b 3b
						2a 19 0b 03 00
						00];
						qcom,use-blink;
						qcom,in-order-command-processing;
				};
	};

};

&mdss_dsi {
	hw-config = "single_dsi";
};
&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_lgd_sw43101_fhd_video>;
	pinctrl-names = "mdss_default", "mdss_sleep";
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active &mdss_vddr_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend &mdss_vddr_suspend>;
	qcom,platform-te-gpio = <&tlmm 10 0>;
	qcom,platform-reset-gpio = <&tlmm 8 0>;
	qcom,platform-enable-gpio = <&tlmm 47 0>;
	qcom,platform-bklight-en-gpio = <&tlmm 48 0>;
};

&mdss_dsi1 {
	pinctrl-names = "mdss_default", "mdss_sleep";
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active &mdss_vddr_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend &mdss_vddr_suspend>;
	qcom,platform-te-gpio = <&tlmm 10 0>;
	qcom,platform-reset-gpio = <&tlmm 8 0>;
	qcom,platform-enable-gpio = <&tlmm 47 0>;
	qcom,platform-bklight-en-gpio = <&tlmm 48 0>;
};

