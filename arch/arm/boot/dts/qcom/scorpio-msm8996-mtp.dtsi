#include "msm8996-xiaomi-common.dtsi"

&pm8994_l21 {
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	qcom,init-voltage = <3300000>;
	regulator-always-on;
};

&pmi8994_haptics {
        status = "ok";
        qcom,wave-play-rate-us = <5000>;
        qcom,vmax-mv = <2830>;
};

&pm8994_gpios {
	gpio@ca00 { /* GPIO 11 - A4 FPC fingerprint ID detect*/
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		status = "okay";
	};
};

